---
layout: nil
---
{% assign updated_time = site.posts | map: 'date' | sort | last %}
<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
<channel>
    <title>Feed Name</title>
    <link>http://domain/</link>
    <atom:link href="http://domain/rss.xml" rel="self" type="application/rss+xml" />
    <description></description>
    <language>en-au</language>
    <pubDate>{{ updated_time | date: "%a, %d %b %Y %H:%M:%S +0900" }}</pubDate>
    <lastBuildDate>{{ updated_time | date: "%a, %d %b %Y %H:%M:%S +0900" }}</lastBuildDate>

    {% for post in site.posts %}
    <item>
      <title>{{ post.title | xml_escape }}</title>
      <link>http://domain{{ post.url }}</link>
      <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S +0900" }}</pubDate>
      <author>Author</author>
      <guid>http://domain{{ post.url }}</guid>
      <description>{{ post.content | xml_escape }}</description>
    </item>
    {% endfor %}

  </channel>
</rss>